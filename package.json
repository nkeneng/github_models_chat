{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "raycast-github-models",
  "title": "Raycast Ã— GitHub Models",
  "description": "Chat with GitHub Models via the GitHub Models API",
  "icon": "icon.png",
  "author": "massimiliano_pasquini",
  "categories": [
    "Productivity"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/MassimilianoPasquini97/raycast_ollama.git"
  },
  "preferences": [
    {
      "name": "githubToken",
      "type": "password",
      "title": "GitHub Token",
      "description": "Personal Access Token for GitHub Models",
      "required": true
    },
    {
      "name": "defaultModel",
      "type": "dropdown",
      "title": "Default Model",
      "description": "Model used by default",
      "required": true,
      "data": [
        {
          "title": "openai/gpt-4o-mini",
          "value": "openai/gpt-4o-mini"
        },
        {
          "title": "meta/llama-3.1-70b-instruct",
          "value": "meta/llama-3.1-70b-instruct"
        }
      ]
    },
    {
      "name": "organization",
      "type": "textfield",
      "title": "Organization",
      "description": "Optional organization for attribution",
      "required": false
    },
    {
      "name": "streaming",
      "type": "checkbox",
      "title": "Streaming",
      "default": true
    },
    {
      "name": "temperature",
      "type": "slider",
      "title": "Temperature",
      "min": 0,
      "max": 2,
      "step": 0.1,
      "default": 0.7
    },
    {
      "name": "maxTokens",
      "type": "textfield",
      "title": "Max Tokens",
      "required": false
    }
  ],
  "commands": [
    {
      "name": "github-models-chat",
      "title": "Chat with GitHub Models",
      "description": "Chat with GitHub Models",
      "mode": "view",
      "path": "src/features/chat/ChatCommand.tsx"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.55.2",
    "@raycast/utils": "^1.9.0",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/node": "18.18.4",
    "@types/react": "18.2.27",
    "eslint": "^7.32.0",
    "prettier": "^2.8.8",
    "typescript": "^4.4.3",
    "ts-node": "^10.9.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "test": "node --loader ts-node/esm --test tests/github-models.test.ts"
  }
}
